{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "awcodes/filament-badgeable-column": "^2.3",
        "bezhansalleh/filament-language-switch": "^3.1",
        "doctrine/dbal": "*",
        "filament/filament": "^3.3",
        "guava/filament-clusters": "*",
        "guzzlehttp/guzzle": "^7.2",
        "jacobtims/inline-date-time-picker": "*",
        "jeffgreco13/filament-breezy": "^2.6",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "leandrocfe/filament-apex-charts": "^3.1",
        "saade/filament-fullcalendar": "^3.0",
        "spatie/laravel-permission": "^6.21",
        "staudenmeir/belongs-to-through": "^2.15",
        "ysfkaya/filament-phone-input": "^3.0"
    },
    "require-dev": {
        "davidbadura/faker-markdown-generator": "^1.1",
        "fakerphp/faker": "^1.9.1",
        "laravel-lang/lang": "^14.0",
        "laravel/breeze": "^1.29",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
